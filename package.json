{
  "name": "@atomicdesign/atomic-singularity",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "author": {
    "name": "Brandon Erb",
    "email": "brandom.m.erb@gmail.com"
  },
  "dependencies": {
    "moment": "^2.29.4"
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "npm run clean && npm run build:full",
    "build:common": "npm run clean && npm run build:swc",
    "build:swc": "swc ./src -d ./dist",
    "build:full": "tsc && tsc-alias"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.92",
    "rimraf": "^5.0.5",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  },
  "files": [
    "dist/**/*.ts",
    "dist/**/*.d.ts",
    "dist/**/*.js"
  ],
  "exports": {
    "./*": "./dist/*",
    "./core": "./dist/systems/index.js",
    "./core/*": "./dist/systems/core/*",
    "./dependency-injection/*": "./dist/systems/dependency-injection/*",
    "./logging": "./dist/systems/logging/index.js"
  }
}
